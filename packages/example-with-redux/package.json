  "dependencies": {
    "react": "16.4.2",
    "@react-qml/renderer": "*",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.12"
  },
  "devDependencies": {
    "@react-qml/cli": "*",
    "remotedev-server": "^0.2.4"
  }
}
