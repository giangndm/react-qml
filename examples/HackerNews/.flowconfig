[ignore]
.*/node_modules/.*/.*test.*
.*bower\.json

[include]
./flow-typed

[libs]

[options]
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$ExpectError

unsafe.enable_getters_and_setters=true
module.ignore_non_literal_requires=true

module.name_mapper='^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$' -> 'identity-obj-proxy'
module.name_mapper='^.+\\.(css|less)(\\?\\w*)?$' -> 'identity-obj-proxy'
module.name_mapper='.*\(.scss\)' -> 'identity-obj-proxy'
